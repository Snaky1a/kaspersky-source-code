        </Control>
        <Control Id="Repair" Type="PushButton" X="$(var.MainDlgNextButtonX)" Y="$(var.MainDlgWizardButtonY)" Width="$(var.MainDlgWizardButtonWidth)" Height="$(var.MainDlgWizardButtonHeight)" Default="yes"
