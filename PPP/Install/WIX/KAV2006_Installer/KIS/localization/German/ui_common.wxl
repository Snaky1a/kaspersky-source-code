	<!-- Product features -->	
	<String Id="AVScannerAndCoreFeatureTitle"><![CDATA[Virensuche]]></String> 
	<String Id="AVScannerAndCoreFeatureDesc"><![CDATA[Die Virensuche bietet die sorgfältige Untersuchung des Computers auf das Vorhandensein gefährlicher Objekte.]]></String>
	<String Id="RtpComponentsFeatureTitle"><![CDATA[Schutzkomponenten]]></String> 
	<String Id="RtpComponentsFeatureDesc"><![CDATA[Die Schutzkomponenten gewährleisten die kontinuierliche Kontrolle über alle Dateien und Programme auf Ihrem Computer. Dadurch kann eine Infektion selbst dann verhindert werden, wenn ein Virus bereits in Ihren Computer eingedrungen ist.]]></String>
	<String Id="FileAntiVirusFeatureTitle"><![CDATA[Datei-Anti-Virus]]></String> 
	<String Id="FileAntiVirusFeatureDesc"><![CDATA[Datei-Anti-Virus untersucht alle Dateien auf dem Computer im Moment des Zugriffs. ]]></String>
	<String Id="MailAntiVirusFeatureTitle"><![CDATA[Mail-Anti-Virus]]></String> 
	<String Id="MailAntiVirusFeatureDesc"><![CDATA[Mail-Anti-Virus untersucht alle eingehenden und ausgehenden E-Mail-Nachrichten auf Viren.]]></String>
	<String Id="WebAntiVirusFeatureTitle"><![CDATA[Web-Anti-Virus]]></String> 
	<String Id="WebAntiVirusFeatureDesc"><![CDATA[Web-Anti-Virus verhindert eine Vireninfektion Ihres Computers durch Daten, die mit dem HTTP-Protokoll übertragen werden.]]></String>
	<String Id="ProactiveDefenseFeatureTitle"><![CDATA[Proaktiver Schutz]]></String> 
	<String Id="ProactiveDefenseFeatureDesc"><![CDATA[Der Proaktiver Schutz erlaubt die Abwehr unbekannter Bedrohungen und die Kontrolle über Veränderungen der Systemregistrierung sowie den Start von Programmen und Makros.]]></String>
	<String Id="AntiSpyFeatureTitle"><![CDATA[Anti-Spy]]></String> 
	<String Id="AntiSpyFeatureDesc"><![CDATA[Anti-Spy gewährleistet die Sicherheit vertraulicher Informationen auf Ihrem Computer.]]></String>
	<String Id="AntiHackerFeatureTitle"><![CDATA[Anti-Hacker]]></String> 
	<String Id="AntiHackerFeatureDesc"><![CDATA[Anti-Hacker kontrolliert alle Netzwerkverbindungen, erkennt Angriffsversuche gegen Ihren Computer und erlaubt es, ihn im Netzwerk für Angriffe unsichtbar zu machen.]]></String>
	<String Id="AntiSpamFeatureTitle"><![CDATA[Anti-Spam]]></String> 
	<String Id="AntiSpamFeatureDesc"><![CDATA[Anti-Spam gewährleistet den Schutz vor unerwünschter Korrespondenz.]]></String>
	<String Id="ParentalControlFeatureTitle"><![CDATA[Kindersicherung]]></String>
	<String Id="ParentalControlFeatureDesc"><![CDATA[Die Kindersicherung gewährleistet die Begrenzung des Zugriffs auf Internetressourcen mit unterwünschtem Inhalt.]]></String>

	<String Id="AdminKitConnectorTitle"><![CDATA[Plugin für Administrationsagent von Kaspersky Administration Kit]]></String>
	<String Id="AdminKitConnectorDesc"><![CDATA[Das Plugin für den Administrationsagenten gewährleistet die Verbindung der Anwendung mit Kaspersky Administration Kit für die entfernte Verwaltung.]]></String>

