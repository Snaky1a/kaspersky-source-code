forward interface Insider;
typedef Insider* hINSIDER;



[
  version(1)
  mnemonic(IID_INSIDER)
  id(59000)
  c_type_name(hINSIDER)
]
interface Insider {

  // ------------------------------------
    error errTASK_NOT_STARTED; // /rus:Возвращается из StartTask, если задача не стартовала
    error errTASK_NOT_RESTARTED; // /rus:Возвращается из RestartTask, если задача не рестартовала


  // ------------------------------------
    method tERROR StartTask( // /rus:Начать выполнение задачи
      const tBYTE* lpTaskData, // /rus:Данные для выполнения задачи.
      tDWORD dwTaskDataSize // /rus:Размер блока данных lpTaskData
    );
    method tERROR StopTask(); // /rus:Остановить выполнение задачи
    method tERROR PauseTask(); // /rus:Приостановить выполнение текущей задачи
    method tERROR ContinueTask(); // /rus:Продолжить выполнение задачи
    method tERROR RestartTask( // /rus:Продолжить выполнение с новыми настройками
      const tBYTE* lpTaskData, // /rus:Данные для выполнения задачи.
      tDWORD dwTaskDataSize // /rus:Размер блока данных lpTaskData
    );


